<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="-2dp"
        android:top="1dp"
        android:bottom="1dp"
        android:right="1dp">
        <shape android:shape="rectangle">
            <stroke
                android:color="@color/colorGrey1"
                android:width="1dp"/>
            <solid android:color="@color/colorCourtSky"/>
            <corners
                android:bottomRightRadius="50dp"
                android:topRightRadius="50dp"/>
        </shape>
    </item>
</layer-list>